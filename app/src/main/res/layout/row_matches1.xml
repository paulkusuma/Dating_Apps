<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:tools="http://schemas.android.com/tools">
    <data>
        <variable name="matches1RowModel" type="com.paulsapplication.app.modules.matches.data.model.Matches1RowModel"/>
    </data>
    <FrameLayout android:id="@+id/frameStackphoto"
        android:layout_height="wrap_content" android:layout_width="@dimen/_140pxh">
        <ImageView android:id="@+id/imagePhotoFour"
            android:layout_gravity="start"
            android:layout_height="@dimen/_200pxh"
            android:layout_width="@dimen/_140pxh"
            android:scaleType="fitXY" cornerRadius="@{@dimen/_15pxh}"
            imageRes="@{@drawable/img_photo_17}"
            tools:ignore="ContentDescription" tools:src="@drawable/img_photo_17"/>
        <LinearLayout android:id="@+id/linearColumnkylecounter"
            android:layout_gravity="bottom|start"
            android:layout_height="wrap_content"
            android:layout_marginTop="@dimen/_10pxv"
            android:layout_width="match_parent" android:orientation="vertical">
            <TextView android:ellipsize="end"
                android:id="@+id/txtKyleCounter"
                android:layout_gravity="start"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_14pxh"
                android:layout_marginStart="@dimen/_14pxh"
                android:layout_width="wrap_content"
                android:singleLine="true"
                android:text="@{matches1RowModel.txtKyleCounter}"
                style="@style/txt" tools:text="@string/lbl_kyle_24"/>
            <FrameLayout android:id="@+id/frameStackphotoOne"
                android:layout_gravity="start"
                android:layout_height="@dimen/_40pxv"
                android:layout_marginTop="@dimen/_5pxv"
                android:layout_width="@dimen/_140pxh" style="@style/groupStyleblack_900cornerRadius">
                <ImageView android:id="@+id/imagePhotoOneFour"
                    android:layout_gravity="start"
                    android:layout_height="@dimen/_40pxh"
                    android:layout_width="@dimen/_140pxh"
                    android:scaleType="fitXY"
                    cornerRadius="@{@dimen/_15pxh}"
                    imageRes="@{@drawable/img_photo_18}"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_photo_18"/>
                <LinearLayout android:id="@+id/linearRowclose"
                    android:layout_gravity="center"
                    android:layout_height="wrap_content"
                    android:layout_marginEnd="@dimen/_25pxh"
                    android:layout_marginStart="@dimen/_29pxh"
                    android:layout_width="match_parent" android:orientation="horizontal">
                    <ImageView android:id="@+id/imageCloseFour"
                        android:layout_height="@dimen/_10pxh"
                        android:layout_marginBottom="@dimen/_15pxv"
                        android:layout_marginTop="@dimen/_15pxv"
                        android:layout_width="@dimen/_10pxh"
                        android:scaleType="fitXY"
                        android:src="@drawable/img_close"
                        tools:ignore="ContentDescription" tools:src="@drawable/img_close"/>
                    <View android:background="@color/white_A700_87"
                        android:id="@+id/lineDivider"
                        android:layout_height="@dimen/_40pxv"
                        android:layout_marginStart="@dimen/_31pxh"
                        android:layout_weight="0.04" android:layout_width="0dp"/>
                    <ImageView android:id="@+id/imageForwardFour"
                        android:layout_height="@dimen/_14pxh"
                        android:layout_marginBottom="@dimen/_12pxv"
                        android:layout_marginStart="@dimen/_26pxh"
                        android:layout_marginTop="@dimen/_13pxv"
                        android:layout_width="@dimen/_16pxh"
                        android:scaleType="fitXY"
                        android:src="@drawable/img_forward"
                        tools:ignore="ContentDescription" tools:src="@drawable/img_forward"/>
                </LinearLayout>
            </FrameLayout>
        </LinearLayout>
    </FrameLayout>
</layout>
