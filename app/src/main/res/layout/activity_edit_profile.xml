                        android:text="@{editProfileVM.editProfileModel.txtDarkmode}"
                        style="@style/txtPoppinsmedium15" tools:text="@string/lbl_darkmode"/>
                </LinearLayout>
                <ImageView android:id="@+id/imageArrowrightFive"
                    android:layout_height="@dimen/_10pxh"
                    android:layout_marginBottom="@dimen/_4pxv"
                    android:layout_marginStart="@dimen/_227pxh"
                    android:layout_marginTop="@dimen/_7pxv"
                    android:layout_width="@dimen/_6pxh"
                    android:scaleType="fitXY"
                    android:src="@drawable/img_arrowright_black_900"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_arrowright_black_900"/>
            </LinearLayout>
            <LinearLayout android:id="@+id/linearRowarrowrightSix"
