                android:paddingBottom="@dimen/_6pxv"
                android:paddingStart="@dimen/_26pxh"
                android:paddingTop="@dimen/_4pxv"
                android:singleLine="true"
                android:text="@{editProfileVM.editProfileModel.txtGroupThirtyOne}"
