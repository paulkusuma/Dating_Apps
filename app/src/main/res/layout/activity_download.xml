<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:tools="http://schemas.android.com/tools">
    <data>
        <variable name="downloadVM" type="com.paulsapplication.app.modules.download.data.viewmodel.DownloadVM"/>
    </data>
    <FrameLayout android:id="@+id/frameDownload"
        android:layout_height="match_parent"
        android:layout_width="match_parent" style="@style/groupStylewhite_A700">
        <LinearLayout android:id="@+id/linearColumndownload"
            android:layout_gravity="top|start"
            android:layout_height="match_parent"
            android:layout_marginHorizontal="@dimen/_14pxh"
            android:layout_marginVertical="@dimen/_14pxv"
            android:layout_width="match_parent" android:orientation="vertical">
            <TextView android:ellipsize="end"
                android:id="@+id/txtDownload"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_10pxh"
                android:layout_width="match_parent"
                android:singleLine="true"
                android:text="@{downloadVM.downloadModel.txtDownload}"
                style="@style/txtAsapromanbold34" tools:text="@string/lbl_download"/>
            <LinearLayout android:id="@+id/linearRow31929761711276"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/_29pxv"
                android:layout_width="match_parent" android:orientation="horizontal">
                <ImageView android:id="@+id/image31929761711276"
                    android:layout_height="@dimen/_114pxh"
                    android:layout_width="@dimen/_114pxh"
                    android:scaleType="fitXY"
                    imageRes="@{@drawable/img_31929761711276}"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_31929761711276"/>
                <FrameLayout android:id="@+id/frameStacksnapinstatwo"
                    android:layout_height="@dimen/_114pxv"
                    android:layout_marginStart="@dimen/_12pxh" android:layout_width="@dimen/_114pxh">
                    <ImageView android:id="@+id/imageSnapinstaTwo"
                        android:layout_gravity="start"
                        android:layout_height="@dimen/_114pxh"
                        android:layout_width="@dimen/_114pxh"
                        android:scaleType="fitXY"
                        imageRes="@{@drawable/img_snapinsta2}"
                        tools:ignore="ContentDescription" tools:src="@drawable/img_snapinsta2"/>
                    <ImageView android:id="@+id/imagePolygonOne"
                        android:layout_gravity="center"
                        android:layout_height="@dimen/_33pxh"
                        android:layout_marginHorizontal="@dimen/_40pxh"
                        android:layout_marginVertical="@dimen/_40pxv"
                        android:layout_width="@dimen/_30pxh"
                        android:scaleType="fitXY"
                        android:src="@drawable/img_polygon1"
                        tools:ignore="ContentDescription" tools:src="@drawable/img_polygon1"/>
                </FrameLayout>
            </LinearLayout>
        </LinearLayout>
    </FrameLayout>
</layout>
